                Console.WriteLine("9 for less than <, 10 for greater than >, 11 for less than or equal to <=");
                Console.WriteLine("12 for greater than or equal to >=, 13 for equal to ==, 14 for assignment +=");
                Console.WriteLine("15 for assignment -=, 16 for assignment /= 17 for assignment *= 18 for assignment %=");
                Console.WriteLine("19 for assignment =, 20 for assignment && 21 for assignment ||, 22 for assignment ?: 23 for !");
